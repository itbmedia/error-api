{
    "name": "itbmedia/error-logger",
    "description": "ITBMedia error bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "ITBMedia"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/yaml": "*",
        "symfony/http-kernel": "4.1.*",
        "symfony/http-client": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "ITBMedia\\ErrorApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ITBMedia\\ErrorApiBundle\\Test\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    }
}