{
    "name": "itbmedia/error-api",
    "description": "Remote server error bundle",
    "keywords": [
        "api",
        "error-handlig"
    ],
    "homepage": "http://www.itbmedia.se",
    "license": "proprietary",
    "require": {
        "php": ">=5.5",
        "symfony/flex": "*",
        "symfony/framework-bundle": "*",
        "symfony/yaml": "*",
        "symfony/http-kernel": "*",
        "symfony/http-client": "*"
    },
    "autoload": {
        "psr-4": {
            "ITBMedia\\ErrorApiBundle\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}